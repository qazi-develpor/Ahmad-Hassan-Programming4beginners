#include <stdio.h>
#include <stdlib.h>  /* for abs() */

void update(int *a, int *b) {
    int orig_a = *a;
    int orig_b = *b;
    *a = orig_a + orig_b;
    *b = abs(orig_a - orig_b);
}

int main() {
    int a, b;
    if (scanf("%d %d", &a, &b) != 2) {
        return 0;
    }
    update(&a, &b);
    printf("%d\n%d\n", a, b);
    return 0;
}
