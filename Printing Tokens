#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    char *s = malloc(1024 * sizeof(char));
    if (s == NULL) return 0;

    // Read the whole line including spaces until newline
    scanf("%[^\n]", s);

    // Use strtok to split by space
    char *token = strtok(s, " ");
    while (token != NULL) {
        printf("%s\n", token);
        token = strtok(NULL, " ");
    }

    free(s);
    return 0;
}
