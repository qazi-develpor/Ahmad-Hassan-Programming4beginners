#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    char s[1001];
    if (scanf("%1000s", s) != 1) {
        return 0;
    }

    int freq[10] = {0};

    size_t len = strlen(s);
    for (size_t i = 0; i < len; i++) {
        char c = s[i];
        if (c >= '0' && c <= '9') {
            int d = c - '0';
            freq[d]++;
        }
    }

    for (int d = 0; d < 10; d++) {
        printf("%d", freq[d]);
        if (d < 9) printf(" ");
    }
    printf("\n");

    return 0;
}
